{
  "language": "ts",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger/plugin",
        "options": {
          "dtoFileNameSuffix": [".dto.ts"]
        }
      },
      {
        "name": "@nartc/automapper-transformer-plugin",
        "options": {
          "modelFileNameSuffix": [".dto.ts", ".entity.ts"]
        }
      }
    ]
  }
}
